{
    "name": "dontdrinkandroot/crud-admin-bundle",
    "type": "symfony-bundle",
    "description": "Generic CRUD Admin with Doctrine Support",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "dontdrinkandroot/common": "^0.2",
        "knplabs/knp-paginator-bundle": "^5.8",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/inflector": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "liip/test-fixtures-bundle": "^2.0",
        "phpunit/phpunit": "^9.5.10",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/validator": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\CrudAdminBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
